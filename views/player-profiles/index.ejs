<%- include('../partials/header') %>
<body>
    <h1>View Player Stats <%= time %></h1>
    <ul>
      <% playerProfiles.forEach(function(player) { %>
        <li>
          
          <a href="/player-profiles/<%= player.id %>">
            <%= player.name %>
          </a>
        </li>
      <% }); %>
    </ul>
    <a href="/player-profiles/new">Add a new player</a>
    <%- include('../partials/footer') %>